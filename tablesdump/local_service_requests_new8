CREATE TABLE local_service_requests_new8 (
    local_service_requests_new8_pk bigint NOT NULL,
    actor text,
    application_detail text[],
    ba_category text,
    ba_segment text,
    bano text[],
    billing_system text,
    bso_key text[],
    bwo_id text[],
    bso_status_key text[],
    bso_error_message_key text[],
    cano text,
    channel text,
    charge_type text,
    complete_dt text,
    destination text[],
    error_message text,
    imsi_key text[],
    last_upd text,
    local_service_requests_s text[],
    location_cd text,
    msisdn_key text[],
    ne_id text,
    order_date timestamp with time zone,
    order_ref text,
    order_status text,
    order_sub_type text[],
    order_type text,
    order_item_status_key text[],
    order_item_error_message_key text[],
    order_item_error_date_key text[],
    pgz_record_type text,
    pgz_task_id text,
    product_name_key text[],
    promotion_action_key text[],
    promotion_code_key text[],
    req_status text,
    request_id text[],
    resp_status text[],
    sano text,
    service_action_key text[],
    service_code_key text[],
    sim_serial_no text,
    submitted_date timestamp with time zone,
    subscriberid1_key text[],
    subscriberid2_key text[],
    subscriberid3_key text[],
    user_id text,
    user_sys text,
    wo_header_error_message_key text[],
    wo_header_status_key text[],
    wo_header_error_date_key text[],
    bso_error_date_key text[],
    int_created_date timestamp with time zone DEFAULT now(),
    int_updated_date timestamp with time zone,
    int_is_deleted text,
    order_info_error_date_new timestamp with time zone,
    order_info_error_date_time timestamp with time zone,
    order_info_error_date timestamp with time zone,
    request_id_int_field text,
    bwo_id_int_field text,
    retry_count_ori_text text,
    val1_key text[],
    val2_key text[],
    val3_key text[],
    val4_key text[],
    val5_key text[],
    val6_key text[],
    val7_key text[],
    val8_key text[],
    val9_key text[],
    val10_key text[],
    val1 text,
    val2 text,
    val3 text,
    val4 text,
    val5 text,
    val6 text,
    val7 text,
    val8 text,
    val9 text,
    val10 text,
    resend_flag text,
    file_name text,
    ref_id text,
    lot_no text,
    val11_key text[],
    val12_key text[],
    val13_key text[],
    val14_key text[],
    val15_key text[],
    val16_key text[],
    val17_key text[],
    val18_key text[],
    val19_key text[],
    val20_key text[],
    retry_count integer
)
DISTRIBUTE BY HASH (local_service_requests_new8_pk);
    START WITH 1
    INCREMENT BY 1
    NO MINVALUE
    NO MAXVALUE
    CACHE 1;
CREATE INDEX gid_actor_local_service_requests_new8 ON local_service_requests_new8 USING gin (bwo_id);
CREATE INDEX gid_destination_local_service_requests_new8 ON local_service_requests_new8 USING gin (destination);
CREATE INDEX gid_order_sub_type_local_service_requests_new8 ON local_service_requests_new8 USING gin (order_sub_type);
CREATE INDEX gid_request_id_local_service_requests_new8 ON local_service_requests_new8 USING gin (request_id);
CREATE INDEX id_actor_local_service_requests_new8 ON local_service_requests_new8 USING btree (actor);
CREATE INDEX id_bwo_id_local_service_requests_new8 ON local_service_requests_new8 USING btree (bwo_id);
CREATE INDEX id_destination_local_service_requests_new8 ON local_service_requests_new8 USING btree (destination);
CREATE INDEX id_mssidn_local_service_request_new8 ON local_service_requests_new8 USING gin (msisdn_key);
CREATE INDEX id_order_ref_local_service_requests_new8 ON local_service_requests_new8 USING btree (order_ref);
CREATE INDEX id_order_sub_type_local_service_requests_new8 ON local_service_requests_new8 USING btree (order_sub_type);
CREATE INDEX id_pgz_record_type_local_service_requests_new8 ON local_service_requests_new8 USING btree (pgz_record_type);
CREATE INDEX id_pgz_task_id_local_service_requests_new8 ON local_service_requests_new8 USING btree (pgz_task_id);
