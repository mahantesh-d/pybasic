CREATE TABLE sap (
    sap_pk bigint NOT NULL,
    int_updated_date timestamp with time zone,
    int_created_date timestamp with time zone DEFAULT now(),
    int_is_deleted text,
    sap_code_key_0 text,
    sap_type_data text,
    sap_account_group_data text,
    parent_sap_code_data text,
    block_unblock_data text,
    hq_sap_code_data text,
    previous_sap_code_data text,
    title_data text,
    name_1_data text,
    name_2_data text,
    name_3_data text,
    name_4_data text,
    addr_1_data text,
    addr_2_data text,
    addr_3_data text,
    addr_4_data text,
    street_data text,
    tumbol_code_data text,
    amphur_code_data text,
    province_code_data text,
    postal_code_data text,
    country_code_data text,
    region_data text,
    sub_region_data text,
    industry_data text,
    transport_zone_data text,
    language_data text,
    search_team_data text,
    corp_group_data text,
    location_desc_data text,
    phone_number_data text,
    phone_ext_data text,
    fax_data text,
    fax_ext_data text,
    created_data timestamp with time zone,
    created_by_data text,
    last_upd_data timestamp with time zone,
    last_upd_by_data text,
    modification_num_data text
)
DISTRIBUTE BY REPLICATION;
