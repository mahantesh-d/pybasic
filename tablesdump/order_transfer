CREATE TABLE order_transfer (
    order_transfer_pk bigint NOT NULL,
    brand_key text[],
    cancel_by text,
    cancel_datetime timestamp with time zone,
    company text,
    distribution_channel text,
    do_no text,
    doc_ref text,
    from_location_code text,
    from_location_name text,
    from_location_province text,
    from_location_region text,
    from_location_sub_region text,
    from_location_subtype text,
    from_location_type text,
    from_substock text,
    mat_code_key text[],
    mat_description_key text[],
    mm_doc_no text,
    mm_doc_year integer,
    model_key text[],
    movement_type text,
    product_subtype_key text[],
    product_type_key text[],
    reason_key text[],
    remark text,
    request_create_by text,
    request_date_time timestamp with time zone,
    request_no_ref text,
    serial_key text[],
    ship_to_address text,
    ship_to_code text,
    ship_to_name text,
    ship_to_province text,
    to_location_code text,
    to_location_name text,
    to_location_province text,
    to_location_region text,
    to_location_sub_region text,
    to_location_subtype text,
    to_location_type text,
    to_substock text,
    transaction_id text,
    transaction_type text,
    transfer_detail text[],
    transfer_detail_s text[],
    transfer_status text,
    vendor_code text,
    vendor_name text,
    int_created_date timestamp with time zone DEFAULT now(),
    int_updated_date timestamp with time zone,
    int_is_deleted text,
    stamp_delivery text,
    shipment_no text,
    shipment_forwarder_flag text,
    shipment_remark text,
    transfer_out_no text,
    transfer_out_createby text,
    transfer_out_create_datetime timestamp with time zone,
    transfer_out_updateby text,
    transfer_out_update_datetime timestamp with time zone,
    transfer_in_no text,
    transfer_in_createby text,
    transfer_in_create_datetime timestamp with time zone,
    transfer_in_updateby text,
    transfer_in_update_datetime timestamp with time zone,
    transfer_no text,
    receive_by text,
    do_no_flag text,
    mm_doc_year_cancel text,
    mm_doc_no_cancel text,
    contact_no text
)
DISTRIBUTE BY HASH (order_transfer_pk);
    START WITH 1
