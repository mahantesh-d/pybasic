CREATE TABLE partner_location (
    partner_location_pk bigint NOT NULL,
    int_updated_date timestamp with time zone,
    int_created_date timestamp with time zone DEFAULT now(),
    int_is_deleted text,
    location_code_key_0 text,
    company_id_key_1 text,
    license_code_key_2 text,
    dist_chn_code_key_3 text,
    chn_sales_code_key_4 text,
    id_type_data text,
    id_no_data text,
    bus_registration_data text,
    name_th_key5 text,
    name_en_key6 text,
    status_data text,
    effective_dt_data timestamp with time zone,
    terminate_dt_data timestamp with time zone,
    remark_data text,
    vat_branch_no_data text,
    vat_branch_name_data text,
    abbrev_key7 text,
    vat_type_data text,
    wt_name_data text,
    wt_address_data text,
    payment_chn_data text,
    payment_chn_status_data text,
    shop_class_data text,
    shop_segment_data text,
    shop_area_data text,
    shop_type_data text,
    renovate_flag_data text,
    row_id_s_b_data text,
    location_id_for_s_b_data text,
    is_opening_mon_data text,
    is_opening_tue_data text,
    is_opening_wed_data text,
    is_opening_thu_data text,
    is_opening_fri_data text,
    is_opening_sat_data text,
    is_opening_sun_data text,
    created_data timestamp with time zone,
    created_by_data text,
    last_upd_data timestamp with time zone,
    last_upd_by_data text,
    modification_num_data text,
    opening_hour_mon_data text,
    closing_hour_mon_data text,
    opening_hour_tue_data text,
    closing_hour_tue_data text,
    opening_hour_wed_data text,
    closing_hour_wed_data text,
    opening_hour_thu_data text,
    closing_hour_thu_data text,
    opening_hour_fri_data text,
    closing_hour_fri_data text,
    opening_hour_sat_data text,
    closing_hour_sat_data text,
    opening_hour_sun_data text,
    closing_hour_sun_data text,
    shop_size_data text,
    retail_shop_data text,
    is_opening_holiday_data text,
    opening_hour_holiday_data text,
    closing_hour_holiday_data text,
    type_code_s_b_data text,
    sub_type_code_s_b_data text,
    business_type_code_s_b_data text,
    characteristic_s_b_data text,
    region_code_key_8 text,
    sale_sub_region_code_key_9 text
)
DISTRIBUTE BY REPLICATION;
