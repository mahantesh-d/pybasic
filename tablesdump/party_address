CREATE TABLE party_address (
    party_address_pk bigint NOT NULL,
    int_updated_date timestamp with time zone,
    int_created_date timestamp with time zone DEFAULT now(),
    int_is_deleted text,
    party_type_data text,
    company_id_key_0 text,
    license_code_key_1 text,
    location_code_key_2 text,
    person_id_key_3 text,
    address_type_data text,
    address_main_flg_data text,
    address_id_key_4 text,
    created_data timestamp with time zone,
    created_by_data text,
    last_upd_data timestamp with time zone,
    last_upd_by_data text,
    modification_num_data text,
    party_address_id_key_5 text
)
DISTRIBUTE BY REPLICATION;
