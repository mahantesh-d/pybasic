    software_version_data text,
    hsdpa_data text,
    hsupa_speed_data text,
    lte_speed_data text,
    infrared_data text,
    wifi_data text,
    wifi_method_data text,
    wifi_tcp_ip_data text,
    cellular_tcp_ip_data text,
    hotspot_20_data text,
    bluetooth_data text,
    design_type_data text,
    finger_scan_data text,
    stylus_pen_data text,
    water_resistant_data text,
    display_color_data text,
    display_resolution_data text,
    display_size_data text,
    camera_data text,
    picture_resolution_data text,
    front_camera_data text,
    memory_in_phone_data text,
    memory_card_slot_data text,
    dual_sim_data text,
    sim_type_data text,
    sim_1000_data text,
    sms_data text,
    mms_data text,
    email_data text,
    ussd_type_data text,
    browser_data text,
    gps_data text,
    nfc_data text,
