    wifi_direct_data text,
    dlna_data text,
    miracast_data text,
    video_call_data text,
    voice_call_data text,
    otg_data text,
    radio_data text,
    tv_tuner_data text,
    tv_out_data text,
    ais_bookstore_data text,
    ais_livetv_data text,
