CREATE TABLE list_of_value (
    list_of_value_pk bigint NOT NULL,
    int_updated_date timestamp with time zone,
    int_created_date timestamp with time zone DEFAULT now(),
    int_is_deleted text,
    lov_type_data text,
    lov_code_key_0 text,
    lov_name_data text,
    language_data text,
    lov_val_0_1_data text,
    lov_val_0_2_data text,
    lov_val_1_1_data text,
    lov_val_1_2_data text,
    lov_val_2_1_data text,
    lov_val_2_2_data text,
    lov_val_2_3_data text,
    lov_val_2_4_data text,
    lov_val_2_5_data text,
    lov_val_2_6_data text,
    lov_val_3_1_data text,
    lov_val_3_2_data text,
    lov_val_4_1_data text,
    lov_val_4_2_data text,
    lov_val_5_1_data text,
    lov_val_5_2_data text,
    lov_val_6_1_data text,
    lov_val_6_2_data text,
    lov_val_7_1_data text,
    active_flg_data text,
    text_desc_data text,
    group_type_data text,
    order_by_data text,
    created_data timestamp with time zone,
    created_by_data text,
    last_upd_data timestamp with time zone,
    last_upd_by_data text,
    modification_num_data text,
    active_flg_key_1 text,
    lov_id_key_3 text
)
DISTRIBUTE BY REPLICATION;
