CREATE TABLE partner_company (
    partner_company_pk bigint NOT NULL,
    int_updated_date timestamp with time zone,
    int_created_date timestamp with time zone DEFAULT now(),
    int_is_deleted text,
    company_id_key_0 text,
    company_abbr_data text,
    vat_branch_no_data text,
    id_type_data text,
    id_no_data text,
    abbrev_data text,
    prefix_company_data text,
    sap_company text,
    sale_org_code_data text,
    invoicing_g_n_v_id text,
    order_by text,
    title_th_data text,
    name_th_data text,
    title_en_data text,
    name_en_data text,
    company_type text,
    status_data text,
    effective_dt_data timestamp with time zone,
    terminate_dt_data text,
    main_phone_type_data text,
    main_phone_number_data text,
    main_phone_ext_data text,
    fax_number_data text,
    remark_data text,
    created_data timestamp with time zone,
    created_by_data text,
    last_upd_data timestamp with time zone,
    last_upd_by_data text,
    modification_num_data text
)
DISTRIBUTE BY REPLICATION;
