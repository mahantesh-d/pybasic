CREATE TABLE sale_out_detail (
    sale_out_detail_pk bigint NOT NULL,
    brand_key text[],
    commercial_name_key text[],
    company text,
    contact_address text,
    contact_no text,
    create_by text,
    create_datetime timestamp with time zone,
    customer_name text,
    document_no text,
    from_substock text,
    location_code text,
    location_code_t2 text,
    location_name text,
    location_province text,
    location_region text,
    location_sub_region text,
    location_subtype text,
    location_type text,
    mat_description_key text[],
    matcode_key text[],
    model_key text[],
    product_subtype_key text[],
    product_type_key text[],
    receipt_ref text,
    sales_code text,
    sales_status text,
    salesoutdetail text[],
    salesoutdetail_s text[],
    serial_key text[],
    shipping_address text,
    sold_status text,
    to_sub_stock text,
    transaction_id text,
    transaction_type text,
    update_by text,
    update_datetime timestamp with time zone,
    int_created_date timestamp with time zone DEFAULT now(),
    int_updated_date timestamp with time zone,
    int_is_deleted text,
    sales_name text,
    sales_pin_id text
)
DISTRIBUTE BY HASH (sale_out_detail_pk);
    START WITH 1
