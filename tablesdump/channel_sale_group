CREATE TABLE channel_sale_group (
    channel_sale_group_pk bigint NOT NULL,
    int_updated_date timestamp with time zone,
    int_created_date timestamp with time zone DEFAULT now(),
    int_is_deleted text,
    license_code_key_0 text,
    company_id_key_1 text,
    dist_chn_code_key_2 text,
    chn_sales_code_key_3 text,
    sap_cus_grp_data text,
    id_type_data text,
    id_no_data text,
    bus_registration_data text,
    name_th_data text,
    name_en_data text,
    status_data text,
    effective_dt_data timestamp with time zone,
    terminate_dt_data text,
    remark_data text,
    abbrev_data text,
    is_opening_mon_data text,
    is_opening_tue_data text,
    is_opening_wed_data text,
    is_opening_thu_data text,
    is_opening_fri_data text,
    is_open_sat_data text,
    is_opening_sun_data text,
    created_data timestamp with time zone,
    created_by_data text,
    last_upd_data timestamp with time zone,
    last_upd_by_data text,
    modification_num_data text,
    opening_hour_mon_data time without time zone,
    closing_hour_mon_data time without time zone,
    opening_hour_tue_data time without time zone,
    closing_hour_tue_data time without time zone,
    opening_hour_wed_data time without time zone,
    closing_hour_wed_data time without time zone,
    opening_hour_thu_data time without time zone,
    closing_hour_thu_data time without time zone,
    opening_hour_fri_data time without time zone,
    closing_hour_fri_data time without time zone,
    opening_hour_sat_data time without time zone,
    closing_hour_sat_data time without time zone,
    opening_hour_sun_data time without time zone,
    closing_hour_sun_data time without time zone,
    is_opening_holiday_data text,
    opening_hour_holiday_data time without time zone,
    closing_hour_holiday_data time without time zone
)
DISTRIBUTE BY REPLICATION;
