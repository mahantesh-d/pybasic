CREATE TABLE stock_brought_forward (
    stock_brought_forward_pk bigint NOT NULL,
    int_updated_date timestamp with time zone,
    int_created_date timestamp with time zone DEFAULT now(),
    int_is_deleted text,
    transaction_id text,
    transaction_type text,
    company text,
    location_code text,
    location_name text,
    location_type text,
    location_sub_type text,
    location_region text,
    location_sub_region text,
    location_province text,
    sub_stock text,
    balance_date date,
    mat_code text,
    mat_type text,
    brand text,
    model text,
    color text,
    commercial_name text,
    mat_description text,
    product_type text,
    product_sub_type text,
    unit text,
    min_quantity integer,
    max_quantity integer,
    reorder_quantity integer,
    balance_qty integer,
    requested_quantity integer,
    receiving_quantity integer,
    sending_quantity integer,
    location_receiving_quantity integer,
    location_sending_quantity integer,
    sold_quantity integer,
    obtain_quantity integer,
    reserve_quantity integer,
    lost_quantity integer,
    adjust_increase_quantity integer,
    adjust_decrease_quantity integer,
    create_by text,
    create_datetime timestamp with time zone,
    update_by text,
    update_datetime timestamp with time zone
)
DISTRIBUTE BY REPLICATION;
