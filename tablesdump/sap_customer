CREATE TABLE sap_customer (
    sap_customer_pk bigint NOT NULL,
    int_updated_date timestamp with time zone,
    int_created_date timestamp with time zone DEFAULT now(),
    int_is_deleted text,
    customer_code_key0 text,
    customer_type_key1 text,
    company_name_data text,
    customer_address_data text,
    contact_name_data text,
    type_data text,
    sub_type_data text,
    region_data text,
    sub_region_data text,
    area_data text,
    zone_data text,
    province_data text,
    net_region_data text,
    house_no_data text,
    building_data text,
    floor_data text,
    mooban_data text,
    soi_data text,
    street_data text,
    tumbol_data text,
    amphur_data text,
    country_data text,
    zipcode_data text,
    distribution_channel_data text,
    account_group_data text,
    division_data text,
    bg_flag_data text,
    title_data text,
    block_flag_data text,
    delete_flag_data text,
    status_data text,
    company_data text,
    taxid_data text,
    branch_id_data text,
    branch_description_data text,
    create_by_data text,
    create_date_time_data timestamp with time zone,
    update_by_data text,
    update_datetime_key2 timestamp with time zone,
    contact_no_data text
)
DISTRIBUTE BY REPLICATION;
