CREATE TABLE product_master (
    matcode_key15 text,
    company_key0 text,
    commercial_name_key1 text,
    mat_description_key2 text,
    brand_key6 text,
    model_key7 text,
    color_key8 text,
    pictureurl1_data text,
    pictureurl2_data text,
    pictureurl3_data text,
    pictureurl4_data text,
    pictureurl5_data text,
    pictureurl6_data text,
    unit_data text,
    mat_type_data text,
    product_type_key3 text,
    product_sub_type_key4 text,
    vendor_code_data text,
    active_flag_data text,
    supplier_code_data text,
    grade_key5 text,
    defect_flag_data text,
    division_data text,
    valuation_class_data text,
    vat_type_data text,
    effective_date_data timestamp with time zone,
    launch_date_data timestamp with time zone,
    priceat_launch_date_data text,
    price_update_data text,
    region_key13 text,
    sub_region_key14 text,
    network_key12 text,
    face_value_key10 text,
    capacity_key9 text,
    expire_date_key11 timestamp with time zone,
    update_datetime_key16 timestamp with time zone,
    update_by_data text,
    create_by_data text,
    create_datetime_data timestamp with time zone,
    product_master_pk bigint NOT NULL,
    int_created_date timestamp without time zone DEFAULT now(),
    int_updated_date timestamp without time zone,
    int_is_deleted text,
    counter_test_field2 integer DEFAULT 0 NOT NULL,
    counter_test_field integer DEFAULT 0 NOT NULL
)
DISTRIBUTE BY HASH (matcode_key15);
