CREATE TABLE channel_sales (
    channel_sales_pk bigint NOT NULL,
    int_updated_date timestamp with time zone,
    int_created_date timestamp with time zone DEFAULT now(),
    int_is_deleted text,
    dist_chn_code_key_0 text,
    chn_sales_code_data text,
    chn_sales_name_data text,
    chn_type_data text,
    menber_category_data text,
    whole_sales_flag_data text,
    distributor_flag_data text,
    retail_flag_data text,
    prefix_loc_data text,
    sync_sap_flag_data text,
    sap_cus_grp_data text,
    remark_data text,
    created_data text,
    created_by_data text,
    last_upd_data timestamp with time zone,
    last_upd_by_data text,
    modification_num_data text,
    type_code_s_b_data text,
    sub_type_code_s_b_data text,
    business_type_code_s_b_data text,
    characteristic_s_b_data text,
    distchn_id_key_4 text
)
DISTRIBUTE BY REPLICATION;
