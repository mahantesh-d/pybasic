CREATE TABLE zip_code (
    zip_code_pk bigint NOT NULL,
    int_updated_date timestamp with time zone,
    int_created_date timestamp with time zone DEFAULT now(),
    int_is_deleted text,
    zip_code_id_key_0 text,
    zip_code_data text,
    province_code_key_1 text,
    country_th_data text,
    amphur_th_data text,
    tumbol_th_data text,
    country_en_data text,
    amphur_en_data text,
    tumbol_en_data text,
    sap_postal_code_data text,
    sap_amphur_code_data text,
    sap_tumbol_code_data text,
    sap_country_code_data text,
    status_data text,
    created_data timestamp with time zone,
    created_by_data text,
    last_upd_data timestamp with time zone,
    last_upd_by_data text,
    modification_num_data text
)
DISTRIBUTE BY REPLICATION;
