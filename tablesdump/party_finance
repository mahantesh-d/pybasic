CREATE TABLE party_finance (
    party_finance_pk bigint NOT NULL,
    int_updated_date timestamp with time zone,
    int_created_date timestamp with time zone DEFAULT now(),
    int_is_deleted text,
    company_id_key_0 text,
    license_code_key_1 text,
    location_code_key_2 text,
    person_id_key_3 text,
    sap_id_key_4 text,
    party_type_data text,
    finance_type_data text,
    inter_comp_data text,
    payment_term_data text,
    payment_method_data text,
    fin_clean_credit_data text,
    fin_credit_limit_data text,
    fin_total_crd_lmt_exp_data text,
    fin_total_crd_lmt_bal_data text,
    fin_percent_total_crd_lmt_use_data text,
    fin_as_of_date_data timestamp with time zone,
    collateral_type_data text,
    collateral_bg_data text,
    bank_name_data text,
    time_of_collateral_data timestamp with time zone,
    collateral_desc_data text,
    collateral_amt_data text,
    receive_dt_data timestamp with time zone,
    expiry_dt_data timestamp with time zone,
    account_no_data text,
    title_data text,
    bank_key_data text,
    account_type_data text,
    account_name_data text,
    branch_name_data text,
    remark_data text,
    created_data timestamp with time zone,
    created_by_data text,
    last_upd_data timestamp with time zone,
    last_upd_by_data text,
    modification_num_data text
)
DISTRIBUTE BY REPLICATION;
