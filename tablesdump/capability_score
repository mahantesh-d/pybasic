CREATE TABLE capability_score (
    capability_score_pk bigint NOT NULL,
    int_updated_date timestamp with time zone,
    int_created_date timestamp with time zone DEFAULT now(),
    int_is_deleted text,
    capability_id_key_0 text,
    survey_id_key_1 text,
    observation_id_key_2 text,
    course_id_key_3 text,
    capability_category_data text,
    capability_type_data text,
    company_id_key_4 text,
    chn_sales_code_key_5 text,
    location_code_key_6 text,
    person_code_key_7 text,
    score_percentage_data text,
    score_data text,
    total_score_data text,
    status_data text,
    score_dt_data text,
    no_of_testing_data text,
    survey_by_first_name_data text,
    survey_by_last_name_data text,
    remark_data text,
    created_data timestamp with time zone,
    created_by_data text,
    last_upd_data timestamp with time zone,
    last_upd_by_data text,
    modification_num_data text
)
DISTRIBUTE BY REPLICATION;
