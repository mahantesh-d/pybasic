CREATE TABLE sap_sale_organization (
    sap_sale_organization_pk bigint NOT NULL,
    int_updated_date timestamp with time zone,
    int_created_date timestamp with time zone DEFAULT now(),
    int_is_deleted text,
    sap_code_key_0 text,
    sap_inter_company_key_1 text,
    cust_group_code_data text,
    dist_channel_code_data text,
    division_data text,
    block_unblock_data text,
    pay_team_data text,
    ship_cond_data text,
    sale_district_data text,
    sale_office_data text,
    sale_group_data text,
    price_group_data text,
    price_list_type_data text,
    pricing_proc_data text,
    price_list_stat_group_data text,
    tax_class_data text,
    currency_data text,
    action_data text,
    created_data timestamp with time zone,
    created_by_data text,
    last_upd_data timestamp with time zone,
    last_upd_by_data text,
    modification_num_data text
)
DISTRIBUTE BY REPLICATION;
