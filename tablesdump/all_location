CREATE TABLE all_location (
    all_location bigint NOT NULL,
    int_updated_date timestamp with time zone,
    int_created_date timestamp with time zone DEFAULT now(),
    int_is_deleted text,
    location_code_key0 text NOT NULL,
    location_name_data text,
    location_province_data text,
    location_district_data text,
    location_subdistrict_data text,
    location_road_data text,
    location_soi_data text,
    location_postcode_data text,
    location_address1_data text,
    location_group_data text,
    location_contactno_data text,
    distribution_channel_data text,
    location_type_data text,
    location_sub_type_data text,
    region_data text,
    sub_region_data text,
    sub_stock_groupid_data text,
    route_group_data text,
    active_flag_data text,
    create_by_data text,
    create_datetime_data timestamp with time zone,
    update_by_data text,
    update_datetime_key1 timestamp with time zone,
    location_sub_group text
)
DISTRIBUTE BY REPLICATION;
