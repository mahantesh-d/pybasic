CREATE TABLE stock_adjustment (
    stock_adjustment_pk bigint NOT NULL,
    adjust_date_time timestamp with time zone,
    adjust_detail text[],
    adjust_detail_s text[],
    adjust_status text,
    adjust_stock_no text,
    adjust_type text,
    brand_key text[],
    commercial_name_key text[],
    company text,
    create_by text,
    create_datetime timestamp with time zone,
    doc_ref text,
    location_code text,
    location_name text,
    location_province text,
    location_region text,
    location_sub_region text,
    location_subtype text,
    location_type text,
    mat_code_key text[],
    mat_description_key text[],
    memo_no text,
    mm_doc_no text,
    mm_doc_year integer,
    model_key text[],
    movement_type text,
    plant text,
    product_subtype_key text[],
    product_type_key text[],
    reason_key text[],
    remark text,
    serial_key text[],
    storage text,
    transaction_id text,
    transaction_type text,
    update_by text,
    update_datetime timestamp with time zone,
    int_created_date timestamp with time zone DEFAULT now(),
    int_updated_date timestamp with time zone,
    int_is_deleted text
)
DISTRIBUTE BY HASH (stock_adjustment_pk);
    START WITH 1
    INCREMENT BY 1
