CREATE TABLE training_course (
    training_course_pk bigint NOT NULL,
    int_updated_date timestamp with time zone,
    int_created_date timestamp with time zone DEFAULT now(),
    int_is_deleted text,
    course_id_key_0 text,
    course_code_data text,
    course_name_data text,
    training_code_data text,
    training_year_data text,
    training_month_data text,
    training_start_time_data timestamp with time zone,
    training_end_time_data timestamp with time zone,
    test_dt_data timestamp with time zone,
    created_data timestamp with time zone,
    created_by_data text,
    last_upd_data timestamp with time zone,
    last_upd_by_data text,
    modification_num_data text
)
DISTRIBUTE BY REPLICATION;
