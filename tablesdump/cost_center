CREATE TABLE cost_center (
    valuation_class_key0 text NOT NULL,
    gl_type_key1 text NOT NULL,
    company_key2 text NOT NULL,
    cost_center_data text,
    description_data text,
    create_by_data text,
    create_datetime_data timestamp with time zone,
    update_by_data text,
    update_datetime_key3 timestamp with time zone,
    cost_center_pk bigint NOT NULL,
    int_created_date timestamp with time zone DEFAULT now(),
    int_updated_date timestamp with time zone,
    int_is_deleted text
)
DISTRIBUTE BY REPLICATION;
